function size([arg1, arg2, arg3]){
  let type=arg1;
  let size1=Number (arg2);
  let size2=Number(arg3);
  let area=0;

  if (type=="square"){
    area=size1*size1
  }else if(type=="rectangle"){
    area=size1*size2;
  }else if(type=="circle"){
    area= Math.PI*size1*size1;
  }else if (type=="triangle"){
    area=size1*size2/2
  }
  console.log(area);
} 
